func bool isPositive(var int a) {
    return a > 0
}

func int add(var int m, var int n) {
    return m + n
}

func int mul(var int m, var int n) {
    return m * n
}

for (var i = 10; isPositive(i); i = i - 1) {
    i
    if (i == 5 && isPositive(i)) {
        for (var ii = i; ii < 100; ii = add(ii, i)) {
            if (true) {
                var ii = false
                ii
            }
            ii
        }
    }
}

